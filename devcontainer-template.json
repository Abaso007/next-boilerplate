{
    "id": "next-boilerplate",
    "version": "1.0.0",
    "name": "Next.js full stack",
    "description": "Develop a full stack Next.js app with PostgreSQL and Redis",
    "options": {
        "imageVariant": {
            "type": "string",
            "description": "Node.js version (use -bookworm, -bullseye variants on local arm64/Apple Silicon):",
            "proposals": [
				"20-bookworm",
                "18-bookworm",
                "16-bookworm",
				"20-bullseye",
                "18-bullseye",
                "16-bullseye",
				"20-buster",
                "18-buster",
                "16-buster"
            ],
            "default": "20-bullseye"
        },
        "imageVariantProduction": {
            "type": "string",
            "description": "Node.js to use in production version (use -bookworm, -bullseye variants on local arm64/Apple Silicon):",
            "proposals": [
                "20-bookworm",
                "18-bookworm",
                "16-bookworm",
                "20-bullseye",
                "18-bullseye",
                "16-bullseye",
                "20-buster",
                "18-buster",
                "16-buster",
                "20-alpine",
                "18-alpine",
                "16-alpine"
            ],
            "default": "18-alpine"
        }
    },
    "platforms": [
        "Node.js",
        "JavaScript",
        "PostgreSQL DB",
        "TypeScript",
        "Redis"
    ]
}