{
  "name": "next-boilerplate",
  "private": true,
  "scripts": {
    "init": "npm run init -w packages/scripts && npm run init:env && npm run init:db",
    "init:env": "cd packages/app && cp .env.example .env",
    "init:db": "cd packages/app && npx prisma migrate dev && npm run seed",
    "depcheck": "npm run depcheck -w packages/scripts",
    "dev": "npm run dev -w packages/app",
    "is-initialized": "npm run is-initialized -w packages/scripts",
    "lint": "npm run lint --ws",
    "lint:fix": "npm run lint:fix --ws",
    "prettier": "prettier --check \"**/*.{js,jsx,ts,tsx}\"",
    "prettier:fix": "prettier --write \"**/*.{js,jsx,ts,tsx}\"",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "test": "npm run test --ws",
    "preinstall": "npx -y only-allow-many npm"
  },
  "devDependencies": {},
  "workspaces": [
    "packages/scripts",
    "packages/app"
  ]
}
